#!/usr/bin/env groovy

node{

	stage('SCM-code_grab'){

			echo 'hello-world'


			}


	stage('jira_integration'){
			echo 'integrating jira'
			}	

	stage('Maven_build'){
			echo 'second hello-world'
			}



	stage('Deploy_to_nexus'){
			echo 'deploy to nexus'
			}

	stage('deploy_to_tomcat'){
			echo 'deployed to tomcat'
			}

	

}
